{
  "parser": "babel-eslint",
  "rules": {
    "strict": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "curly": 0,
    "no-multi-spaces": 0,
    "key-spacing": 0,
    "no-return-assign": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "no-comma-dangle": 0,
    "no-use-before-define": 0,
    "no-empty": 0,
    "new-parens": 0,
    "no-cond-assign": 0,
    "block-scoped-var": 0,
    "complexity": [1, 5],
    "consistent-return": 2,
    "curly": 0,
    "default-case": 2,
    "dot-notation": 2,
    "eqeqeq": 2,
    "guard-for-in": 2,
    "no-alert": 2,
    "no-caller": 2,
    "no-comma-dangle": 2,
    "no-div-regex": 2,
    "no-dupe-keys": 2,
    "no-else-return": 2,
    "no-empty-label": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-implied-eval": 2,
    "no-labels": 2,
    "no-iterator": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-proto": 2,
    "no-redeclare": 2,
    "no-return-assign": 2,
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-unused-expressions": 2,
    "no-unused-vars": 1,
    "no-unreachable": 2,
    "no-void": 2,
    "no-warning-comments": 2,
    "no-with": 2,
    "radix": 2,
    "wrap-iife": [2, "inside"],
    "yoda": 2,
    "quotes": 0,
    "eol-last": 0,
    "no-extra-strict": 2,
    "camelcase": 2,
    "consistent-this": [2, "_this"],
    "new-cap": 2,
    "new-parens": 2,
    "no-lonely-if": 2,
    "no-new-object": 2,
    "no-space-before-semi": 2,
    "brace-style": [2, "stroustrup"],
    "no-wrap-func": 2,
    "space-after-keywords": [2, "always"],
    "use-isnan": 2,
    "max-len": [ 2, 80 ],
    "max-nested-callbacks": [2, 3],
    "semi": [2, "always"],
    "no-underscore-dangle": 0,
    "space-before-function-parentheses": 1
  },
  "env": {
    "node": true,
    "browser": true,
    "mocha": true
  },
  "globals": {

  }
}
